<script type="text/javascript">
	$(function() { $(".datepicker").datepicker({
    	format: 'mm/dd/yyyy'
    }); });
</script>

<%= form_for @challenge do |f| %>
  <p>
	<%= f.label :title %>
	<%= f.text_field :title %>
	<%= f.label :start_time %>
	<%= f.text_field :start_time, :class => "datepicker" %>
	<%= f.label :end_time %>
	<%= f.text_field :end_time, :class => "datepicker" %>
	<%= f.label :description %>
	<%= f.text_field :description %>
  </p>
  <%= f.fields_for :webpages do |builder| %>
  	  <%= render 'webpage_fields', :f => builder %>
	<% end %>
  <%= f.submit "Submit" %>

<% end %>